<p id="notice"><%= notice %></p>

<h1>Listing Profile Introductions</h1>

<table>
  <thead>
    <tr>
      <th>Profile</th>
      <th>Body</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @profile_introductions.each do |profile_introduction| %>
      <tr>
        <td><%= profile_introduction.profile_id %></td>
        <td><%= profile_introduction.body %></td>
        <td><%= link_to 'Show', profile_introduction %></td>
        <td><%= link_to 'Edit', edit_profile_introduction_path(profile_introduction) %></td>
        <td><%= link_to 'Destroy', profile_introduction, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Profile introduction', new_profile_introduction_path %>
