<h1>Core Members page</h1>

<h2>Asana tasks</h2>

<% @tasks['data'].each do |task| %>
    <p>Title: <%= task['name'] %></p>
    <p>Assigned to: <%= task['assignee']['name'] %></p>
    <p>Description: <%= task['notes'] %></p>
    <p>Completed: <%= task['completed'] %></p>
    <p>Modified at: <%= task['modified_at'] %></p>
    <p>Due on<%= task['due_on'] %></p>
    <br />
<% end %>


