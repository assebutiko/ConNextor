<p id="notice"><%= notice %></p>

<h1>Listing Project User Classes</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @project_user_classes.each do |project_user_class| %>
      <tr>
        <td><%= project_user_class.name %></td>
        <td><%= link_to 'Show', project_user_class %></td>
        <td><%= link_to 'Edit', edit_project_user_class_path(project_user_class) %></td>
        <td><%= link_to 'Destroy', project_user_class, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Project user class', new_project_user_class_path %>
