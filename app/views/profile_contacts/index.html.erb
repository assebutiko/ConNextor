<p id="notice"><%= notice %></p>

<h1>Listing Profile Contacts</h1>

<table>
  <thead>
    <tr>
      <th>Profile</th>
      <th>Type</th>
      <th>Name</th>
      <th>Link</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @profile_contacts.each do |profile_contact| %>
      <tr>
        <td><%= profile_contact.profile_id %></td>
        <td><%= profile_contact.type %></td>
        <td><%= profile_contact.name %></td>
        <td><%= profile_contact.link %></td>
        <td><%= link_to 'Show', profile_contact %></td>
        <td><%= link_to 'Edit', edit_profile_contact_path(profile_contact) %></td>
        <td><%= link_to 'Destroy', profile_contact, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Profile contact', new_profile_contact_path %>
