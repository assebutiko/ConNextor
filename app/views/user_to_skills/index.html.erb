<p id="notice"><%= notice %></p>

<h1>Listing User To Skills</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Skill</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user_to_skills.each do |user_to_skill| %>
      <tr>
        <td><%= user_to_skill.user %></td>
        <td><%= user_to_skill.skill %></td>
        <td><%= link_to 'Show', user_to_skill %></td>
        <td><%= link_to 'Edit', edit_user_to_skill_path(user_to_skill) %></td>
        <td><%= link_to 'Destroy', user_to_skill, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User to skill', new_user_to_skill_path %>
