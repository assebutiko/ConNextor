<p id="notice"><%= notice %></p>

<h1>Listing Profile Experiences</h1>

<table>
  <thead>
    <tr>
      <th>Profile</th>
      <th>Name</th>
      <th>Position</th>
      <th>From date</th>
      <th>To date</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @profile_experiences.each do |profile_experience| %>
      <tr>
        <td><%= profile_experience.profile_id %></td>
        <td><%= profile_experience.name %></td>
        <td><%= profile_experience.position %></td>
        <td><%= profile_experience.from_date %></td>
        <td><%= profile_experience.to_date %></td>
        <td><%= profile_experience.description %></td>
        <td><%= link_to 'Show', profile_experience %></td>
        <td><%= link_to 'Edit', edit_profile_experience_path(profile_experience) %></td>
        <td><%= link_to 'Destroy', profile_experience, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Profile experience', new_profile_experience_path %>
