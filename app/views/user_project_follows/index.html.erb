<p id="notice"><%= notice %></p>

<h1>Listing User Project Follows</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Project</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user_project_follows.each do |user_project_follow| %>
      <tr>
        <td><%= user_project_follow.user_id %></td>
        <td><%= user_project_follow.project_id %></td>
        <td><%= link_to 'Show', user_project_follow %></td>
        <td><%= link_to 'Edit', edit_user_project_follow_path(user_project_follow) %></td>
        <td><%= link_to 'Destroy', user_project_follow, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User project follow', new_user_project_follow_path %>
